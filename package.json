  "scripts": {
    "build": "tsc -b && vite build && node -e \"const fs = require('fs'); const path = require('path'); const clientDir = path.join('dist', 'client'); if (fs.existsSync(clientDir)) { fs.readdirSync(clientDir).forEach(file => { const src = path.join(clientDir, file); const dest = path.join('dist', file); if (!fs.existsSync(dest)) fs.renameSync(src, dest); }); fs.rmSync(clientDir, { recursive: true, force: true }); }\"",
    "cf-typegen": "wrangler types",
    "check": "tsc && vite build && wrangler deploy --dry-run",
    "dev": "vite",
    "dev:all": "concurrently --kill-others \"npm run dev\" \"npm run server\"",
    "knip": "knip",
    "lint": "eslint .",
    "start": "node server.js",
    "preview": "serve -s dist",
    "server": "node server.js"
  }
